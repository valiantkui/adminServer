<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"      
 "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">

<!-- namespace映射文件的namespace必须等于Mapper映射器的全限定名 -->
<mapper namespace="dao.P_sDao">
	
	
	<insert id="addP_s" parameterType="entity.P_s" >
		insert into p_s values(#{p_no},#{s_no}) 
	</insert>
	
	<delete id="deleteByS_no" parameterType="String">
		delete from p_s where p_s.s_no = #{s_no}
	</delete>
	
	
	
</mapper>







