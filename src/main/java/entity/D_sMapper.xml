<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"      
 "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">

<!-- namespace映射文件的namespace必须等于Mapper映射器的全限定名 -->
<mapper namespace="dao.D_sDao">
	
	
	<insert id="addD_s" parameterType="entity.D_s" >
		insert into d_s values(#{d_no},#{s_no}) 
	</insert>
	
	<delete id="deleteByD_no" parameterType="String">
		delete from d_s where d_no = #{d_no}
	</delete>
	
	
	
</mapper>







